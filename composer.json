{
  "name": "ikitiki/pgq",
  "description": "PGQ Consumer for PHP",
  "keywords": ["postgresql", "pgq"],
  "version": "1.0.0",
  "type": "library",
  "homepage": "http://github.com/ikitiki/pgq",
  "repositories" : [ {
    "type" : "composer",
    "url" : "https://packagist.org/"
  } ],
  "authors": [
    {
      "name": "Murat Kabilov",
      "email": "murat@kabilov.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.5.0",
    "ext-pgsql": "*",
    "ext-json": ">=1.0.0",
    "ikitiki/pgdb": ">=1.0.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.6",
    "ikitiki/pgdb": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Ikitiki\\": "src/Ikitiki"
    }
  },
  "archive": {
    "exclude": [ "/tests" ]
  }
}